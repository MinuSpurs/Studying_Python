day['일요일']='Sun'
print(day)
----------------------------------------------------------------------------------------------
day={'월요일':'Mon','화요일':'Tue','수요일':'Wed','목요일':'Thu','금요일':'Fri'}
day['토요일']='Sat'
day['일요일']='SUN'
