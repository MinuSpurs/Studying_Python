i=1
print('369 369~ 369 369~')

while i<101:
    say=input()
    if '3' in str(i) or '6' in str(i) or '9' in str(i):
        if say!='짝':
            print('틀렸습니다.')
            break
    else:
        if say!=str(i):
            print('틀렸습니다1.')
            break
    i+=1
