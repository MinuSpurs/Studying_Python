print('구구단을 외자! 구구단을 외자!')

for i in range(5):
    a,b=random.randint(2,9),random.randint(1,9)
    que=str(a)+'x'+str(b)+'?'
    ans=int(input(que))
    if ans!=a*b:
        j=1
        print('틀렸습니다! 5번 반복해 읽어봅시다.')
        while j<6:
            print('{} {}'.format(que,a*b))
            j+=1
